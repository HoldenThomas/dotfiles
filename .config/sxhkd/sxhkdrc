	pamixer -{t,d,i} 2; kill -44 $(pidof dwmblocks); kill -44 $(pidof i3blocks)
XF86MonBrightness{Up,Down}
	brightnessctl s {+10%,10%-}
super + Escape
  syscontrol.sh
super + d
  dmenu_run
super + Return
	$TERMINAL
super + w
	$BROWSER
super + shift + w
  $BROWSER --incognito
super + e
  pcmanfm
super + shift + e
  $TERMINAL -e lf
super + x
	keepmenu
super + shift + x
  keepassxc
super + a
	$TERMINAL -e pulsemixer
super + shift + a
	$TERMINAL -e nmtui
