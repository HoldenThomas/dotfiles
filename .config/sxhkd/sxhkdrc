XF86Audio{Mute,LowerVolume,RaiseVolume}
	pamixer -{t,d,i} 2; kill -44 $(pidof dwmblocks)
XF86MonBrightness{Up,Down}
	brightnessctl s {+10%,10%-}
super + {w,e,p,m,z}
	{$BROWSER,pcmanfm,keepassxc,dmenumount,dmenuunicode}
super + shift + {l,m,z}
	{slock,dmenuumount,displayset}
super + Return
	$TERMINAL
super + a
	$TERMINAL -e pulsemixer
super + shift + a
	$TERMINAL -e nmtui
